##############
## Xft settings
##############

Xft.dpi:                    120
Xft.antialias:              true
Xft.rgba:                   rgb
Xft.hinting:                true
Xft.hintstyle:              hintslight


! Dracula Xresources palette
*.foreground: #F8F8F2
!*.foreground:  #90ACBC
! *.background: #282A36

!*.background: #0C0A20
*.background: #17181B
*.color0:     #000000
*.color8:     #4D4D4D
*.color1:     #FF5555
*.color9:     #FF6E67
*.color2:     #50FA7B
*.color10:    #5AF78E
*.color3:     #F1FA8C
*.color11:    #F4F99D
*.color4:     #BD93F9
*.color12:    #CAA9FA
*.color5:     #FF79C6
*.color13:    #FF92D0
*.color6:     #8BE9FD
*.color14:    #9AEDFE
*.color7:     #BFBFBF
*.color15: #E6E6E6

URxvt*scrollBar:         true
URxvt*scrollBar_right:   true

URxvt*transparent:       false

URxvt*font: xft:ibm3270:Regular:size=11
URxvt*letterSpace: 0

Rxvt.perl-ext-common: default,matcher
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1

*antialias:  true
*autohint:   true

URxvt.scrollBar: false

urxvt.perl-ext-common: default,resize-font,clipboard

URxvt.keysym.Control-Meta-c: builtin-string:
URxvt.keysym.Meta-Left: builtin-string:

URxvt.clipboard.autocopy: true
URxvt.keysym.Shift-Control-V: eval:paste_clipboard



